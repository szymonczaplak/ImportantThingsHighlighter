{
  "manifest_version": 2,
  "name": "Important Things Highlighter 1.0",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
        "http://127.0.0.1:8000/",
        "tabs"
    ]

}